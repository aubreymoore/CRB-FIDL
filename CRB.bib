@article{etebari_complete_2020,
	title = {Complete genome sequence of Oryctes rhinoceros nudivirus isolated from the coconut rhinoceros beetle in Solomon Islands},
	issn = {0168-1702},
	url = {http://www.sciencedirect.com/science/article/pii/S0168170219308123},
	doi = {10.1016/j.virusres.2020.197864},
	abstract = {Oryctes nudivirus (species {OrNV}) has been an effective biocontrol agent against the insect pest Oryctes rhinoceros (Coleoptera: Scarabaeidae) for decades, but there is evidence that resistance could be evolving in some host populations. We detected {OrNV} infection in O. rhinoceros from Solomon Islands and used Oxford Nanopore Technologies ({ONT}) long-read sequencing to determine the full length of the virus genomic sequence isolated from an individual belonging to a mitochondrial lineage ({CRB}-G) that was previously reported as resistant to {OrNV}. The complete circular genome of the virus consisted of 125,917 nucleotides, 1.698 bp shorter than the originally-described full genome sequence of Ma07 strain from Malaysia. We found 130 out of 139 previously annotated {ORFs} (seven contained interrupted/non-coding sequences, two were identified as duplicated versions of the existing genes), as well as a putatively inverted regions containing four genes. These results demonstrate the usefulness of a long-read sequencing technology for resolving potential structural variations when describing new virus isolates. While the Solomon Islands isolate exhibited 99.41\% nucleotide sequence identity with the originally described strain, we found several genes, including a core gene (vlf-1), that contained multiple amino acid insertions and/or deletions as putative polymorphisms of large effect. Our complete annotated genome sequence of a newly found isolate in Solomon Islands provides a valuable resource to help elucidate the mechanisms that compromise the efficacy of {OrNV} as a biocontrol agent against the coconut rhinoceros beetle.},
	pages = {197864},
	journaltitle = {Virus Research},
	shortjournal = {Virus Research},
	author = {Etebari, Kayvan and Filipović, Igor and Rašić, Gordana and Devine, Gregor J. and Tsatsia, Helen and Furlong, Michael J.},
	urldate = {2020-01-22},
	date = {2020-01-13},
	langid = {english},
	keywords = {Scarabaeidae, Coconut Rhinoceros Beetles, Genome, Oryctes nudivirus (species ), Oxford Nanopore, Solomon Islands, Technologies ({ONT}) sequencing},
	file = {ScienceDirect Snapshot:/home/aubrey/snap/zotero-snap/common/Zotero/storage/VFMVCYJR/S0168170219308123.html:text/html;Submitted Version:/home/aubrey/snap/zotero-snap/common/Zotero/storage/5A5ZVXXK/Etebari et al. - 2020 - Complete genome sequence of Oryctes rhinoceros nud.pdf:application/pdf},
}

