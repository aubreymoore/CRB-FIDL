@article{laurence_beaudoin-ollivier_flight_2003,
	title = {Flight movement of Scapanes australis australis(Boisduval) (Coleoptera: Scarabaeidae: Dynastinae) in Papua New Guinea: a radiotelemetry study},
	volume = {42},
	abstract = {We used radiotelemetry and/or chemical light-tags to track the flight of 15 individuals of 
 
