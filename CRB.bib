	journaltitle = {Annales de l'Institut Pasteur / Virologie},
	shortjournal = {Annales de l'Institut Pasteur / Virologie},
	author = {Gourreau, J. M. and Kaiser, C. and Monsarrat, P.},
	urldate = {2020-12-11},
	date = {1982-10-01},
	langid = {english},
	keywords = {Baculovirus, Biological control, Innocuité, Innocuousness, Lutte biologique, Mouse, Souris},
	file = {ScienceDirect Snapshot:/home/aubrey/snap/zotero-snap/common/Zotero/storage/AECXRUEP/S0769261782800695.html:text/html},
