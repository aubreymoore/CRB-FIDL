	journaltitle = {Revista mexicana de ciencias forestales},
	author = {Quiroz, E. J. and Morales, O. M. and Ramírez, O. T. and Villalobos, G. G. and Alarcón, M. E. G. and Nolazquez, O. C.},
	urldate = {2020-05-15},
