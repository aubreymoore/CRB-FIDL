	note = {Publisher: Taylor \& Francis
\_eprint: https://doi.org/10.1080/19420889.2020.1774310},
	keywords = {Oryctes rhinoceros, Cycas micronesica, Adaptive radiation, {CRB}, ecological fitting hypothesis, host shift, starch},
	file = {Full Text PDF:/home/aubrey/snap/zotero-snap/common/Zotero/storage/PX6JIMVB/Marler et al. - 2020 - Burrowing activity of coconut rhinoceros beetle on.pdf:application/pdf;Snapshot:/home/aubrey/snap/zotero-snap/common/Zotero/storage/XQQW3DTT/19420889.2020.html:text/html},
