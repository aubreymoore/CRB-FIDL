@article{pansuwan_s._incidence_2019,
	title = {Incidence of coconut rhinoceros beetle in decomposed oil palm empty fruit bunches and control strategy by Metarhizium guizhouense {PSUM}04},
	volume = {47},
	abstract = {Oil palm farmer usually use oil palm empty fruit bunches ({EFB}) that come out of the mill as waste are used as an organic fertilizer for oil palms. This material is a breeding site for immature stage of coconut rhinoceros beetle (Oryctes rhinoceros) that a serious pest of oil palms. This research study was measured physical and biological changes in different decomposition times of {EFB} and incidence of immature stages of O. rhinoceros. The {pH}, temperature and humidity in 1-6 months of decomposed {EFB} was 6.00-7.17, 31.67-39.00°C and 16.80-23.00\%, respectively, whereas 7-12 months of decomposed {EFB} was 5.33-5.92, 29.16-31.50°C and 23.42-29.10\%, respectively. The 1st and 2nd instar larvae of O. rhinoceros were initially found at 9 months of decomposed {EFB} and all stages of immature larvae (1st, 2nd and 3rd) were found at 10 months of decomposed {EFB}. The decomposed {EFB} at 11 and 12 months showed the highest number of 2nd and 3rd instar larvae with 26.3±1.9 and 40.0±5.8 larvae, respectively. The control strategy of immature stages of O. rhinoceros by Metarhizium guizhouense {PSUM}04 showed the percentage mortality of 1st, 2nd and 3rd instar larvae with 93.3±3.8, 95.6±2.2 and 75.6±5.9\%, respectively, after treated 35 days.},
	pages = {923--930},
	issue = {No.Suppl. 1},
	journaltitle = {Kaen Kaset = Khon Kaen Agriculture Journal},
	author = {Pansuwan, S. and Kalanuson, K. and Anothai, J. and Thaochan, N.},
	date = {2019},
}

