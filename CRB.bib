	note = {Publisher: Instituto Nacional de Investigaciones Forestales, Agrícolas y Pecuarias},
	file = {Full Text PDF:/home/aubrey/snap/zotero-snap/common/Zotero/storage/Z65HW3Z7/Jiménez Quiroz et al. - 2017 - Primera intercepción del escarabajo rinoceronte as.pdf:application/pdf;Snapshot:/home/aubrey/snap/zotero-snap/common/Zotero/storage/C5D4NN4J/scielo.html:text/html},
