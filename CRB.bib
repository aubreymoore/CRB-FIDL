@article{jackson_natural_2022,
	title = {Natural disasters lead to {CRB} attack in the Pacific Islands},
	abstract = {Volcanic eruptions, tsunamis and cyclones affecting the Pacific islands severely damage coconut palms and other vegetation leaving a vast amount of organic debris which is an ideal breeding ground for the coconut rhinoceros beetles ({CRB}) (Oryctes rhinoceros). This will lead to a surge in numbers of the damaging adult beetle one to two years after the disaster and can severely impact the recovery period. A management plan for {CRB} should be incorporated into recovery plans following natural disasters in the Pacific to avoid the threat to food security and economic recovery.},
	author = {Jackson, Trevor},
	date = {2022-02-01},
	file = {Full Text PDF:/home/aubrey/snap/zotero-snap/common/Zotero/storage/7H2HGBIJ/Jackson - 2022 - Natural disasters lead to CRB attack in the Pacifi.pdf:application/pdf},
}

@article{pradipta_potency_2020,
	title = {The Potency of Collecting Larvae of Oryctes rhinoceros L. (Coleoptera:Scarabaeidae) in the Oil Palm Plantation},
	volume = {42},
	doi = {10.17503/agrivita.v42i1.2489},
	shorttitle = {The Potency of Collecting Larvae of Oryctes rhinoceros L. (Coleoptera},
	abstract = {The intensive and continuous collection of rhinoceros beetles, Oryctes rhinoceros L. (Coleoptera: Scarabaeidae), is an extraordinary strategy in {IPM} (Integrated Pest Management). This study was intended to evaluate the potency of people in collecting larvae and to determine the age structure of the beetle population. The study was conducted in Asahan Regency, North Sumatera Province. Adult men, adult women, adolescent girls, and adolescent boys collected beetles for 2 hours in each breeding site. Results showed that the beetle was more significant abundant in decomposed trunks at plots of immature and mature palms than at heaps of the empty fruit bunches namely 236, 221 and 93 individuals/2 hours. The population age-structure showed significantly predominant of 2nd and 3rd instars in all breeding sites, i.e. eggs, 1st instars, 2nd instars, and 3rd instars, pupae, adult males, and adult females were 2.02, 23.29, 21.39, 47.06, 3.17, 0.86, and 2.21\%, respectively. Adult men, adult women, and adolescent boys had similar potency in collecting 3rd instar larvae while the least amount was collected by the adolescent girls viz. 70, 85, 60, and 38 larvae/2 hours, respectively. It is suggested that the study results support future efforts in the sustainable control of rhinoceros beetle through collecting larvae for commercial utilization.},
	journaltitle = {{AGRIVITA} Journal of Agricultural Science},
	shortjournal = {{AGRIVITA} Journal of Agricultural Science},
	author = {Pradipta, Anugerah and Wagiman, F. and Witjaksono, Witjaksono},
	date = {2020-02-01},
	file = {Full Text PDF:/home/aubrey/snap/zotero-snap/common/Zotero/storage/UYAB3P7C/Pradipta et al. - 2020 - The Potency of Collecting Larvae of Oryctes rhinoc.pdf:application/pdf},
}

@article{moore_detecting_2022,
	title = {Detecting Coconut Rhinoceros Beetle Breeding Sites Using Harmonic Radar},
	volume = {3},
	url = {https://doi.org/10.3897/arphapreprints.e86423},
	doi = {10.3897/arphapreprints.e86423},
	abstract = {Coconut rhinoceros beetle, a major pest of coconut and oil palms, is causing severe economic and environmental damage following recent invasions of several Pacific islands. Population suppression and eradication of this pest requires location and destruction of active and potential breeding sites where all life stages aggregate. Three search tactics for discovering breeding sites have been used with limited success: visual search by humans, search with assistance from detector dogs, and search by tracking {CRB} adults fitted with radio transmitters.Here we suggest a fourth search tactic: releasing {CRB} adults fitted with harmonic radar tags to locate breeding sites. Our idea is to find static end points for tags which accumulate at breeding sites, rather than active tracking of individual beetles. We plan to use commercially available hand-held harmonic radar devices. If we are successful, this technique may be useful for locating other insects which aggregate, such as hornets and other social insects.},
	pages = {ARPHA Preprints},
	journaltitle = {{ARPHA} Preprints},
	author = {Moore, Aubrey and Siderhurst, Matthew},
	date = {2022},
	file = {Moore and Siderhurst - 2022 - Detecting Coconut Rhinoceros Beetle Breeding Sites.pdf:/home/aubrey/snap/zotero-snap/common/Zotero/storage/SU9I952L/Moore and Siderhurst - 2022 - Detecting Coconut Rhinoceros Beetle Breeding Sites.pdf:application/pdf},
}

